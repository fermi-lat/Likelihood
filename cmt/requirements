package Likelihood

# $Id: requirements,v 1.1.1.1 2003/01/30 23:23:02 burnett Exp $

use astro  v*
use cfitsio v1* IExternal

private

# allow use of internal path
macro_prepend cppflags "-I ../src " WIN32 "/I..\src "

macro includes "" WIN32 "*.h"
library Likelihood -no_share $(includes) *.cxx

# test application
application test_Likelihood test/*.cxx